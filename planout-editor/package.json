{
  "name": "planout-editor",
  "version": "0.0.1",
  "description": "Reactive PlanOut interface",
  "main": "js/app.js",
  "dependencies": {
    "es6-promise": ">=0.1.1",
    "underscore": "~1.6.0",
    "codemirror": "~4.4.0",
